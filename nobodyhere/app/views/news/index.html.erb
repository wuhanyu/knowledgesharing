<h1>求职信息</h1>

<table class="table table-bordered table-striped center-table">
  <tr>
    <th>标题</th>
    <th>获取时间</th>
  </tr>

<% @news.each do |news| %>
  <tr>
    <td><a href="<%= news.web_urls %>" target="_blank"><%= news.title %></a></td>
    <td><%= news.created_at %></td>
  </tr>
<% end %>
</table>

<br />
